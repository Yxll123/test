.container {
  display: flex;
  flex-direction: column;
  min-height: 100%;
  height: 100%;
}

page {
  height: 100%;
  font-family: "Helvetica Neue", Arial, "PingFang SC", "Microsoft YaHei", sans-serif;
  background-color: #eee;
}

.textSymbol {
  font-family: Arial;
  color: #f40;
  font-size: 28rpx;
  /* font-weight: 600; */
}

.textContent {
  /* font-family: 宋体; *//* font-weight: 600; */
  font-size: 36rpx;
}

.textGray {
  color: #aaa;
  font-size: 30rpx;
}

.address-content-one {
  font-size: 28rpx;
  color: #000;
}

.address-content-two {
  font-size: 28rpx;
  color: #aaa;
}


/* 商品名 */

.good-name {
  /* font-family: 宋体; */
  font-size: 32rpx;
  font-weight: 600;
}

.good-name-one {
  /* font-family: 宋体; */
  font-size: 30rpx;
}

/* 商品价格 */

.good-price {
  font-size: 36rpx;
  color: #f40;
}

.good-price-icon {
  font-size: 28rpx;
  color: #f40;
}

/*整个页面置灰*/

.mask {
  width: 750rpx;
  height: 100%;
  background: #c0c0c0;
  z-index: 10;
  position: fixed;
  top: 0;
  opacity: 0.7;
}

/*选择规格层面*/

.choose {
  /* flex-direction: column; */
  align-items: center;
  background-color: #fff;
  position: fixed;
  bottom: 0;
  /* right: 0; *//* border: 1px solid #f44336; */
  z-index: 100;
  width: 100%;
  box-sizing: border-box;
}

.choose .spec-img-container {
  height: 220rpx;
  border-bottom: 2px solid #f5f5f5;
  display: flex;
  flex-direction: row;
}

.choose .spec-img-container 
image {
  width: 180rpx;
  height: 180rpx;
  margin-top: -32rpx;
  margin-left: 32rpx;
  margin-right: 32rpx;
  border: 2px #f5f5f5 solid;
  border-radius: 5px;
}

.choose .spec-img-container 
.middle {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.choose .spec-img-container 
.name {
  margin-top: 20rpx;
  margin-right: 20rpx;
}

.choose .spec-img-container 
.price {
  margin-bottom: 20rpx;
}

.choose .spec-img-container 
.clear {
  margin-top: 20rpx;
  margin-right: 10rpx;
}

.choose .spec-view-container {
  width: 100%;
  border-bottom: 2px solid #f5f5f5;
}

.choose .spec-view-container
.modal-de-select {
  font-size: 28rpx;
  margin-left: 20rpx;
  margin-top: 10rpx;
}

.choose .spec-view-container
.modal-spec {
  margin-left: 40rpx;
  margin-top: 20rpx;
  font-size: 28rpx;
  overflow: hidden;
  min-height: 80rpx;
}

.choose .spec-view-container
.modal-spec 
  ul {
  width: 100%;
  height: 100%;
}

.choose .spec-view-container
.modal-spec 
  li {
  width: 40%;
  float: left;
  margin-right: 20rpx;
  margin-bottom: 10rpx;
}

.choose .spec-view-container
.modal-spec-item- {
  border: 2rpx #00007b solid;
  color: #00007b;
  font-size: 30rpx;
  margin-bottom: 10rpx;
  border-radius: 25rpx;
  text-align: center;
  width: 80%;
  height: 40rpx;
  line-height: 40rpx;
}

.choose .spec-view-container
.modal-spec-item-0 {
  border: 2rpx #ccc solid;
  color: #ccc;
  font-size: 30rpx;
  margin-bottom: 10rpx;
  border-radius: 25rpx;
  text-align: center;
  width: 80%;
  height: 40rpx;
  line-height: 40rpx;
}

.choose .spec-view-container
.modal-spec-item-selected {
  color: #fff;
  border: 2rpx #f00 solid;
  background-color: #f00;
  font-size: 30rpx;
  margin-bottom: 10rpx;
  border-radius: 25rpx;
  text-align: center;
  width: 80%;
  height: 40rpx;
  line-height: 40rpx;
}

.choose .spec-view-container
.modal-spec-item-1 {
  color: #D32323;
  border: 2rpx #D32323 solid;
  background-color: #FFE6E6;
  font-size: 30rpx;
  margin-bottom: 10rpx;
  border-radius: 25rpx;
  text-align: center;
  width: 80%;
  height: 40rpx;
  line-height: 40rpx;
}

.choose .buy-num {
  margin-top: 10rpx;
  margin-bottom: 20rpx;
  font-size: 28rpx;
}

.choose .buy-num
.font-num {
  margin-left: 20rpx;
}

/*主容器*/

.stepper {
  width: 218rpx;
  height: 62rpx;
  /*给主容器设一个边框*/
  border: 2rpx solid #ccc;
  border-radius: 50rpx;
  margin-left: 40rpx;
  margin-top: 20rpx;
}

/*加号和减号*/

.stepper text {
  width: 60rpx;
  height: 100%;
  line-height: 62rpx;
  text-align: center;
  float: left;
}

/*数值*/

.stepper input {
  width: 90rpx;
  height: 62rpx;
  float: left;
  text-align: center;
  font-size: 32rpx;
  /*给中间的input设置左右边框即可*/
  border-left: 2rpx solid #ccc;
  border-right: 2rpx solid #ccc;
}

/*普通样式*/

.stepper .normal {
  color: black;
}

/*禁用样式*/

.stepper .disabled {
  color: #ccc;
}

/*知道从来点击来时候 */

.choose .modal_button {
  flex-direction: row;
  background-color: #c62127;
  position: inherit;
  bottom: 0;
  z-index: 100;
  width: 750rpx;
  height: 106rpx;
  line-height: 106rpx;
  color: #fff;
  text-align: center;
  margin: 0 auto;
}

.choose .modal-button-two {
  display: flex;
  flex-direction: row;
  align-items: center;
  /* float: left; */
  background-color: #fff;
  /* position: inherit; *//* bottom: 0; *//* right: 0; *//* z-index: 10; */
  width: 100%;
  /* height: 50px; */
}

.choose .modal-button-two
button {
  border-radius: 0px;
}

.choose .modal-button-two
.modal-type- {
  width: 50%;
}

.choose .modal-button-two
.modal-type-0 {
  margin-top: 20rpx;
  width: 100%;
}

.choose .modal-button-two
.modal-type-1 {
  display: none;
}

.global-add-cart {
  background-color: #F8B555;
  font-size: 32rpx;
  color: #fff;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
}

.global-immediate-buy {
  background-color: #D32323;
  font-size: 32rpx;
