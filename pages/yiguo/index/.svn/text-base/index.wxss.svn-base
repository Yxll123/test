/*footer*/

.footer {
  text-align: center;
  height: 100rpx;
  margin-top: 20rpx;
}

.footer view {
  width: 700rpx;
}

.footer text {
  display: flex;
  justify-content: center;
  align-items: center;
  color: #aaa;
  font-size: 12px;
}

.rush-buy{
  background-color: #F67A7A;
  color: #fff;
  font-size: 30rpx;
  height: 54rpx;
  line-height: 54rpx;
  width: 160rpx;
  border-radius: 27rpx;
  text-align: center;
}

.percent-detail {
  color: #aaa;
  font-size: 24rpx;
  margin-right: 30rpx;
}

.percent {
  width: 150rpx;
  height: 10rpx;
  border: 2rpx solid #F67A7A;
  border-radius: 10rpx;
  overflow: hidden;
}

.seckill-act-img {
  width: 750rpx;
