page {
  height: 100%;
  width: 100%;
}

/*导航/轮播图容器*/

.swiper-box {
  height: 380rpx;
  /* border-bottom: 50rpx solid #eee; */
   background-color: #fff; 
}

.slide-image {
  display: block;
  height: 380rpx;
  width: 100%;
}

.home-modules {
  background-color: #fff;
  width: 100%;
  display: flex;
  flex-direction: row;
  margin-top: 20rpx;
}

.sub-module {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 10rpx 0rpx;
}

.sub-module-image {
  width: 160rpx;
  height: 160rpx;
  margin-bottom: 10rpx;
}

.sub-module-name {
  font-size: 28rpx;
}

.hot-container {
  background: #fff; 
  min-height: 550rpx;
  margin-top: 20rpx;
}

.hot-goods-title {
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 100rpx;
  justify-content: center;
  align-items: center;
}

.txt {
  color: #000;
  margin: 0rpx 20rpx;
}

.line {
  width: 40px;
  border-top: 1px solid #ccc ;
}

.good {
  display: flex;
  padding: 32rpx;
  border-top: 2rpx solid #eee;
}

.good .good-img {
  width: 240rpx;
  height: 240rpx;
  margin-right: 32rpx;
}

.good .good-info {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

/*商品信息*/

.top {
  width: 100%;
  font-size: 34rpx;
  font-weight: 600;
}

.bottom {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

/* 价格 */

.good-current-price {
  font-size: 34rpx;
  color: red;
  /* padding-top: 10rpx;
  padding-bottom: 10rpx; */
}

/* 添加 */

.good-add {
  width: 60rpx;
  height: 60rpx;
  margin-right: 10rpx;
  margin-bottom: 10rpx;
}

