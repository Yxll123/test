}

.seckill-act-time {
  display: flex;
  flex-direction: row;
  padding-left: 400rpx;
  padding-top: 80rpx;
}

.seckill-act-time .time{
  height: 50rpx;
  line-height: 50rpx;
  padding: 0rpx 4rpx 0rpx 4rpx;
  margin: 0rpx 6rpx 0rpx 6rpx;
  background-color: #000;
