page {
  height: 100%;
}

.swiper-container {
  position: relative;
}

.swiper-container .swiper {
  height: 750rpx;
}

.swiper-container .swiper .img {
  width: 100%;
  height: 100%;
}

.swiper-container .dots {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.swiper-container .dots .dot {
  margin: 0 8rpx;
  height: 14rpx; 
  border-radius: 8rpx;
  transition: all 0.6s;
}

.dot.none {
 width: 14rpx;
 background: #ccc;
}

.dot.active {
  width: 24rpx;
  background: #f00;
}

.circle {
  width: 30rpx;
  height: 30rpx;
  margin: 0 8rpx;
  border-radius: 50%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.circle.none {
  background-color: #c0c0c0;
  opacity: 0.7;
}

.circle.active {
  background-color: #f00;  
}

.triangle {
  width: 0;
  height: 0;
  border-width: 8rpx 0 8rpx 14rpx;
  border-style: solid;
}

.triangle.none {
  border-color: transparent transparent  transparent #fff; /*透明 透明 透明 灰*//* position:relative; */
}

.triangle.active {
  border-color: transparent transparent  transparent #fff; /*透明 透明 透明 灰*//* position:relative; */
}

.video-icon-active {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  background-color: #c0c0c0;
  opacity: 0.7;
  position: absolute;
  left:325rpx;
  top:325rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.video-icon-none {
  display: none;
}

.video-triangle-icon {
  width: 0;
  height: 0;
  border-width: 20rpx 0 20rpx 34.6rpx;
  border-style: solid;
  border-color: transparent transparent  transparent #fff;
  /* position: absolute;
  left: 34rpx;
  top:30rpx; */
}

.video-time {
  font-size: 20rpx;
}

.video-class-show {
  display: block;
  position: absolute;
  width: 100%;
  height: 750rpx;
  top: 224rpx;
  background-color: #000;
}

.video-class-hide {
  display: none;
}

.video {
  width: 100%;
  height: 680rpx; 
}

.video-stop {
  background-color: #ccc;
  height: 50rpx;
  width: 180rpx;
  border-radius: 25rpx;
  color: #000;
  font-size: 28rpx;
  line-height: 50rpx;
  text-align: center;
  margin: 0 auto;
}












