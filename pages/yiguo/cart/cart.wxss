page {
  height: 100%;
  width: 100%;
}

.cart-box {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.cart-img {
  margin-bottom: 60rpx;
}

.cart-img image {
  height: 200rpx;
  width: 200rpx;
}

.cart-btn {
  height: 80rpx;
  width: 200rpx;
  line-height: 80rpx;
  text-align: center;
  border-radius: 40rpx;
  border: 2rpx solid #495a7b;
}

.cart-emt {
  margin-bottom: 30rpx;
}

.top-title {
  height: 90rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  border-bottom: 2rpx solid #eef;
  background-color: #fff;
}

.top-title .title-left {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.icon-circle {
  margin-left: 20rpx;
  margin-right: 20rpx;
}

