/* 编辑 */

.edit-content {
  position: absolute;
  right: 0rpx;
  display: flex;
  flex-direction: row;
  height: 220rpx;
}

.edit-content .stepper {
  /* margin: 0rpx; */
  margin-bottom: 20rpx;
  margin-right: 10rpx;
  align-self: flex-end;
}

.edit-delete {
  height: 220rpx;
  line-height: 220rpx;
  text-align: center;
  background-color: #f67a7a;
  width: 90rpx;
  color: #fff;
}

.footer-button
.submit-not {
  background-color: #dbdbdb;
  color: #7f7f7f;
}

.middle-list {
  display: flex;
  justify-content: space-between;
  width: 100%;
  overflow: hidden;
}

/*******/

.list-good {
  height: 200rpx;
  width: 100%;
  /* box-sizing:border-box; */
  border-bottom: 2rpx solid #eef;
  padding: 10rpx 20rpx 10rpx 20rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  background-color: #fff;

  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  -webkit-transform: translateX(90px);
  transform: translateX(90px);
  margin-left: -90px
}

.list-good .gond-icon {
  height: 100%;
  display: flex;
  /* justify-content: center; */
  align-items: center;
}

.list-good .good-content {
  flex: 1;
  height: 100%;
  margin-left: 20rpx;
  margin-right: 20rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.good-content .content-pic {
  width: 160rpx;
  height: 160rpx;
}

.good-content .good-content-right {
  height: 100%;
  flex: 1;
  margin-left: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.list-good .good-num {
  height: 100%;
  width: 60rpx;
  text-align: center;
  margin-top: 10rpx;
}

.touch-move-active .list-good,
.touch-move-active .del {
 -webkit-transform: translateX(0);
 transform: translateX(0);
}

.del {
   background-color: orangered;
  width: 90px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #fff;
  -webkit-transform: translateX(90px);
  transform: translateX(90px);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
}


.number-choose-view {
  position: absolute;
  right: 32rpx;

  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-end;
  height: 200rpx;
  width: 160rpx;

  /* background-color: #F8F8F8;
  border-radius: 26rpx; */
}
.number-choose-view view {
  height: 52rpx;
  width: 160rpx;
  display: flex;
  flex-direction: row;
  background-color: #F8F8F8;
  border-radius: 26rpx;
}
.number-choose-view image {
  height: 52rpx;
  width: 52rpx;
}
.number-choose-view input {
  flex: 1;
  font-size: 24rpx;
  display: flex;
  flex-direction: row;
  align-self: center;
  /* justify-content: center; */
   text-align: center; 
}

.not-buy-view{
  border-radius: 10rpx;
  background-color: #eee;
  color: #999;
  font-size: 24rpx;
  margin-top: 10rpx;
}