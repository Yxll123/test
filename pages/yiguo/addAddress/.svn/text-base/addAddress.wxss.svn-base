page {
  height: 100%;
  background-color: #ededed;
  font-family: "Helvetica Neue",
                 "Hiragino Sans GB",
                 "Microsoft YaHei",
                 "\9ED1\4F53",
                 Arial,
                 sans-serif;
}

.addressForm {
  margin-top: 10rpx;
}

.input_box {
  height: 90rpx;
  background-color: #fff;
  border-bottom: 2rpx solid #ededed;
  display: flex;
  align-items: center;
}

.input_title {
  font-size: 34rpx;
  padding-left: 30rpx;
  width: 160rpx;
}

.input_detail {
  font-size: 32rpx;
  padding-left: 20rpx;
  display: flex;
  flex-direction: row;
  align-items: center
}

.input_detail input {
  width: 480rpx;
}

.defalutRegionTip {
  color: #808080
}

.switch_box {
  height: 90rpx;
  font-size: 34rpx;
  background-color: #fff;
  display: flex;
  align-items: center;
}
.switch_title {
  width: 160rpx;
  padding-left: 30rpx;
}
.switch {
  padding-left: 20rpx;
}

.button_submit {
  margin: 60rpx 60rpx 0 60rpx;
  border-radius: 50%;
}
.button_submit button {
  background-color: #D32323;
  color: #fff;
}

.unAuthorizedView {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top: 260rpx;
}
.unAuthorizedView image {
  width: 450rpx;
  height: 450rpx;
}
.unAuthorizedView button {
  margin-top: 20rpx;
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 4rpx;
  background-color: #D32323;
  font-size: 28rpx;
  color: #ffffff;
}
