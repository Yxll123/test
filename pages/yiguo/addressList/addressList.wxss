.add_address{
  position: fixed;
  bottom: 0;
  font-size: 34rpx;
  height: 100rpx;
  width: 100%;
  display: flex;
  background-color: #D32323
}
.add-content{
  display: flex;
  width: 330rpx;
  margin-left: 210rpx;
  text-align: center;
}

.icon-add-50{
    position: relative;
    box-sizing: border-box;
    width: 50px;
    height: 50px;
}
.icon-add-50:before{
    content: '';
    position: absolute;
    width: 20px;
    height: 1px;
    left: 50%;
    top: 50%;
    margin-left: -10px;
    margin-top: -0.5px;
    background-color: #ffffff;
}
.icon-add-50:after{
    content: '';
    position: absolute;
    width: 1px;
    height: 20px;
    left: 50%;
    top: 50%;
    margin-left: -0.5px;
    margin-top: -10px;
    background-color: #ffffff;
}
.add_address_tips{
  margin-left: 10rpx;
  padding-top: 26rpx;
  color: #ffffff;
}