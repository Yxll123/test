page{
  height: 100%;
  background-color: #ededed;
}
.list-tab{
  height: 106rpx;
  box-sizing: border-box;
  padding: 5px 10px;
  border-bottom: 18rpx solid #ededed;
  background-color: #ffffff;
}
.list-tab ul{
  display: flex;
  height: 72rpx;
}
.list-tab li{
  flex: 1;
  display: flex;
  font-size: 15px;
  justify-content: center;
  align-items: center;
  color: #7F7F7F;
}
.list-tab .active{
  color: #000000;
  font-weight:bold;
  border-bottom: 5rpx solid #000000;
}
.orderlist-box {
  padding: 5px 10px;
  border-bottom: 10px solid #ededed;
  background-color: #ffffff;
}
.order_firstline {
  font-size: 30rpx;
  height: 60rpx;
  border-bottom: 5rpx solid #ededed;
  display: flex;
}
.order_time {
  line-height: 60rpx;
  width: 580rpx;
}
.order_status {
  line-height: 60rpx;
  color: #FF4400
}
.goodslist-box {
  padding-top: 10rpx;
  border-bottom: 5rpx solid #ededed;
  display: flex
}
.goods-images image{
  width: 160rpx;
  height: 160rpx;
}
.goods-info {
  width: 510rpx;
  margin-left: 20rpx;
}
.name {
  height: 120rpx;
}
.spuName {
  font-size: 32rpx;
}
.skuName {
  margin-top: 10rpx;
  font-size: 26rpx;
  color: #7F7F7F
}
.priceAndAmount {
  color: #FF4400;
  font-size: 30rpx;
  height: 50rpx;
  display: flex;
  align-items: center
}
.amount {
  padding-left: 340rpx;
}
.orderPrice {
  padding-top: 10rpx;
  padding-right: 10rpx;
  text-align: right;
  border-bottom: 5rpx solid #ededed;
}
.orderPrice_payAmt {
  color: #FF4400;
  font-size: 32rpx;
  padding-bottom: 10rpx;
}
.orderPrice_discountAmt {
  font-size: 26rpx;
}
.navigator_operate {
  height: 80rpx;
  /* border-bottom: 5rpx solid #ededed; */
  line-height: 80rpx;
  display: flex
}
.navigator_button {
  margin-top: 15rpx;
  margin-left: 540rpx;
  font-size: 30rpx;
  height: 60rpx;
  width: 160rpx;
  background-color: #D32323;
  color: #fff;
  border-radius: 10rpx;
  border: 2rpx solid #D32323; 
  text-align: center;
  line-height: 60rpx;
}
.navigator_button1 {
  margin-top: 15rpx;
  margin-left: 340rpx;
  font-size: 30rpx;
  height: 60rpx;
  width: 160rpx;
  background-color:  #F8B555;
  color: #fff;
  border-radius: 10rpx;
  border: 2rpx solid #F8B555; 
  text-align: center;
  line-height: 60rpx;
}
.navigator_button2 {
  margin-top: 15rpx;
  margin-left: 40rpx;
  font-size: 30rpx;
  height: 60rpx;
  width: 160rpx;
  background-color: #D32323;
  color: #fff;
  border-radius: 10rpx;
  border: 2rpx solid #D32323; 
  text-align: center;
  line-height: 60rpx;
